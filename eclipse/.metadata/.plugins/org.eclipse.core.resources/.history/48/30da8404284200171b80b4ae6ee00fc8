	}

	@Override
	public void processCommand(ICommandSender p_71515_1_, String[] lines) {
		if(lines.length > 0){
			if(lines.length == 1 && lines[0].equals("reload")){
				
			}
		}
		else
        {
            throw new WrongUsageException("Must be write a task!", new Object[0]);
        }
	}

}
